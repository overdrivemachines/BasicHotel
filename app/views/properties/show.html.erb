<p id="notice"><%= notice %></p>

<h1>Property Settings</h1>

<p>
  <strong>Name:</strong>
  <%= @property.name %>
  <br>
  <% if (@address != nil) %>
    <strong>Address 1:</strong>
    <%= @address.address1 %>
    <br>
    <strong>Address 2:</strong>
    <%= @address.address2 %>
    <br>
    <strong>City:</strong>
    <%= @address.city %>
    <br>
    <strong>State:</strong>
    <%= @address.state %>
    <br>
    <strong>Zip:</strong>
    <%= @address.zip %>
    <br>
    <strong>Country:</strong>
    <%= @address.country %>
    <br>
    <strong>Phone:</strong>
    <%= @address.phone %>
    <br>
  <% end %>

  <strong>Access code:</strong>
  <%= @property.access_code %>
  <br>
  <strong>Notes:</strong>
  <%= @property.notes %>
</p>

<%= link_to 'Edit', edit_property_path(@property) %>